   <to-view-id>/paginas/tipoAcomodacao/cadastro.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>listarTipoAcomodacoes</from-outcome>
   <to-view-id>/paginas/tipoAcomodacao/listagem.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>listarClientes</from-outcome>
   <to-view-id>/paginas/cliente/listagem.xhtml</to-view-id>
  </navigation-case>
