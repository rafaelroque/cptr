   <from-outcome>cadastrarUsuario</from-outcome>
   <to-view-id>/paginas/usuario/cadastro.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>listarUsuario</from-outcome>
   <to-view-id>/paginas/usuario/listagem.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>listagemCurso</from-outcome>
   <to-view-id>/paginas/curso/listagem.xhtml</to-view-id>
  </navigation-case>
  <!-- FIM PROJETO -->
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <resource-bundle>
   <base-name>message</base-name>
   <var>msg</var>
  </resource-bundle>
  <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
 </application>
</faces-config>
