  </navigation-case>
  <navigation-case>
   <from-outcome>listarCidadaos</from-outcome>
   <to-view-id>/paginas/cidadao/listagem.xhtml</to-view-id>
  </navigation-case>
  
  
  <!-- MATERIAL CONSUMO -->
  <navigation-case>
   <from-outcome>cadastrarMaterialConsumo</from-outcome>
   <to-view-id>/paginas/materialConsumo/cadastro.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>listarMaterialConsumo</from-outcome>
   <to-view-id>/paginas/materialConsumo/listagem.xhtml</to-view-id>
